<div class="navbar navbar-default navbar-inverse navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <img src="/assets/parkingclub8.png">
      <div class="nav-collapse collapse navynav">
        <div class="pull-right navnav">
          <% if logged_in? %>
            <%= link_to "#{current_user.first_name} #{current_user.last_name}", user_path(id: current_user.id), class: 'btn btn-default' %>
            <%= link_to 'Logout', logout_path, method: :delete, class: 'btn btn-primary' %>
          <% else %>
            <%= link_to 'Log In', login_path, class: 'btn btn-success' %>
            <%= link_to 'Sign Up', new_user_path, class: 'btn btn-primary' %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
