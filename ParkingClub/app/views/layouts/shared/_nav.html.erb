<div class="navbar navbar-default navbar-inverse navbar-fixed-top navtop">
  <div class="navbar-inner navtop1">
    <div class="container">
      <img src="/assets/parkingclub8.png">
      <div class="nav-collapse collapse navynav">
        <ul class="nav">
          <li class="divider-vertical"></li>
          <li class="active"><%= link_to "Home", root_path %></li>
        </ul>
        <div class="pull-right navnav">
          <% if logged_in? %>
            <%= link_to "#{current_user.first_name} #{current_user.last_name}", user_path(id: current_user.id), class: 'btn btn-default' %>
            <%= link_to 'Logout', logout_path, method: :delete, class: 'btn btn-default logout-btn' %>
          <% else %>
            <%= link_to 'Log In', login_path, class: 'btn btn-default login-btn' %>
            <%= link_to 'Sign Up', new_user_path, class: 'btn btn-default signup-btn' %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
