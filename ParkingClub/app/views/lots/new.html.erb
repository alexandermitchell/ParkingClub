<%= form_for @lot do |f| %>

<%= f.label :name %><br>
<%= f.text_field :name %>

<%= f.label :address %><br>
<%= f.text_field :address %>

<%= f.label :city %><br>
<%= f.text_field :city %>

<%= f.label :province %><br>
<%= f.text_field :province %>

<%= f.label :country %><br>
<%= f.text_field :country %><br>

<%= f.submit "Activate your parking lot" %>

<%end%>